    "cloudinary": "^1.22.0",
    "cloudinary-react": "^1.6.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "env": "0.0.2",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "forever-monitor": "^3.0.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^1.6.5",
    "nodemon": "^2.0.7",
    "npm": "^7.18.1",
    "passport": "^0.4.0",
